<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.attendencetrackingapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/attendence"
        android:label="@string/app_name"
        android:roundIcon="@drawable/attendence"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".update_popup"></activity>
        <activity android:name=".update_attendence" />
        <activity android:name=".show_attendence" />
        <activity android:name=".creator" />
        <activity android:name=".signup" />
        <activity android:name=".Listview" />
        <activity android:name=".subject" />
        <activity android:name=".screen2" />
        <activity android:name=".screen" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>